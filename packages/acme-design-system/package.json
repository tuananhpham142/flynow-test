{
    "name": "@acme/design-system",
    "version": "0.0.1",
    "repository": "https://github.com/vercel/examples.git",
    "license": "MIT",
    "scripts": {
        "dev": "rm -rf dist && pnpm build:swc -w",
        "build": "rm -rf dist && pnpm build:swc && pnpm types",
        "build:swc": "pnpm copy-tailwind && swc src -d dist",
        "copy-tailwind": "mkdir dist && cp src/tailwind.cjs dist/tailwind.cjs",
        "types": "tsc --emitDeclarationOnly",
        "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
        "clean": "rm -rf ./dist && rm -rf .turbo"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./*": [
            "./dist/*.js",
            "./dist/*/index.js"
        ],
        "./tailwind": {
            "require": "./dist/tailwind.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "typesVersions": {
        "*": {
            "*": [
                "src/*",
                "src/*/index"
            ],
            "tailwind": [
                "dist/tailwind.d.cts"
            ]
        }
    },
    "publishConfig": {
        "typesVersions": {
            "*": {
                "*": [
                    "dist/*.d.ts",
                    "dist/*/index.d.ts"
                ],
                "tailwind": [
                    "dist/tailwind.d.cts"
                ]
            }
        }
    },
    "peerDependencies": {
        "next": "*",
        "react": "^18.0.0-0",
        "react-dom": "^18.0.0-0"
    },
    "dependencies": {
        "@acme/api": "workspace:*",
        "@floating-ui/react": "^0.24.8",
        "@swc/helpers": "^0.5.1",
        "@types/lodash": "^4.14.195",
        "clsx": "^1.2.1",
        "lodash": "^4.17.21",
        "dayjs": "1.11.7",
        "react-hook-form": "^7.45.1",
        "react-number-format": "^5.2.2",
        "react-select": "^5.7.3",
        "swiper": "^9.3.2"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.53",
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.5",
        "chokidar": "^3.5.3",
        "eslint": "^8.38.0",
        "eslint-config-acme": "workspace:*",
        "typescript": "5.0.4"
    }
}
